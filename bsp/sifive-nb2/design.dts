/include/ "core.dts"
/ {
	chosen {
		metal,entry = <&testram0 0 0>;
		metal,boothart = <&L10>;
		metal,eccscrub = <0>;
		metal,itim = <&L9 0 0>;
		metal,ram = <&L19 0 0>;
	};
};
&L21 {
	testram0: testram@20000000 {
		compatible = "sifive,testram0";
		reg = <0x20000000 0x1fffffff>;
		reg-names = "mem";
	};
};
&L20 {
	testram1: testram@40000000 {
		compatible = "sifive,testram0";
		reg = <0x40000000 0x1fffffff>;
		reg-names = "mem";
	};
};
